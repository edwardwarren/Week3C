#include <iostream>

using std::cout;
using std::cin;
using std::endl; //program uses endl

//function main begins program execution
int main() {
    int number1{0};
    int number2{0};

    cout << "Enter two integers to compare: ";
    cin >> number1 >> number2; //read two integers from user

    if (number1 == number2) {
    cout << number1 << " == " << number2 << endl;
    }

    if (number1 != number2) {
    cout << number1 << " != " <<number2 << endl;
    }

    if (number1 < number2) {
    cout << number1 << " < " <<number2 << endl;
}
    if (number1 > number2) {
        cout << number1 << " > " <<number2 << endl;
    }

    if (number1 <= number2) {
        cout << number1 << " <= " <<number2 << endl;
    }

    if (number1 >= number2) {
        cout << number1 << " >= " <<number2 << endl;
    }
}
